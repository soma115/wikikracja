[Unit]
Description=demo daphne daemon
After=network.target

[Service]
User=r1
Group=nginx
WorkingDirectory=/var/www/demo
ExecStart=/var/www/prod-env/bin/daphne --bind 0.0.0.0 --port 9000 zzz.asgi:application

[Install]
WantedBy=multi-user.target
